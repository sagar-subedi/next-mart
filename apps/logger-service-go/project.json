{
    "name": "logger-service-go",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/logger-service-go",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "go build -o dist/logger-service-go ./cmd/main.go",
                "cwd": "apps/logger-service-go"
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "go run cmd/main.go",
                "cwd": "apps/logger-service-go"
            },
            "configurations": {
                "development": {
                    "env": {
                        "PORT": "6016",
                        "KAFKA_GROUP_ID": "log-events-group-go"
                    }
                }
            },
            "defaultConfiguration": "development"
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "golangci-lint run",
                "cwd": "apps/logger-service-go"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "go test ./...",
                "cwd": "apps/logger-service-go"
            }
        }
    },
    "tags": [
        "go",
        "service"
    ]
}